{
    "name": "fife",
    "description": "Finance for Everyone",
    "author": "Luke Hurst <hurstlj@umich.edu",
    "type": "module",
    "version": "0.11.2",
    "scripts": {
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro}\" \"test/**/*.ts\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,astro}\" \"test/**/*.ts\"",
        "lint:fix": "npm run lint -- --fix",
        "check": "npx tsc --noEmit",
        "test": "vitest --run --coverage",
        "test:watch": "vitest --coverage",
        "preflight": "npm run format && npm run lint && npm run check && npm run test",
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro"
    },
    "dependencies": {
        "@astrojs/alpinejs": "^0.4.7",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@types/alpinejs": "^3.13.11",
        "alpinejs": "^3.14.9",
        "astro": "^5.7.8",
        "bulma": "^1.0.3",
        "oidc-client-ts": "^3.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@types/node": "^22.15.2",
        "@vitest/coverage-v8": "^3.1.1",
        "astro-eslint-parser": "^1.2.2",
        "csslint": "^1.0.5",
        "eslint": "^9.24.0",
        "eslint-plugin-astro": "^1.3.1",
        "eslint-plugin-import": "^2.31.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.5.3",
        "prettier-plugin-astro": "^0.14.1",
        "typescript-eslint": "^8.29.1",
