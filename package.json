    },
    "devDependencies": {
        "@eslint/js": "^9.24.0",
        "@vitest/coverage-v8": "^3.1.1",
        "astro-eslint-parser": "^1.2.2",
        "eslint": "^9.24.0",
        "eslint-plugin-astro": "^1.3.1",
        "eslint-plugin-import": "^2.31.0",
        "prettier": "^3.5.3",
        "prettier-plugin-astro": "^0.14.1",
        "typescript-eslint": "^8.29.1",
        "vitest": "^3.1.1"
